<?xml version="1.0"?>
<robot  xmlns:xacro="http://www.ros.org/wiki/xacro">

  <!--Define constants-->
  <xacro:property name="pi" value="3.141592654"/>
  <xacro:property name="deg" value="0.017453293"/><!--degrees to radians-->
  <xacro:property name="mass0" value="150" />
  <xacro:property name="mass1" value="138" />
  <xacro:property name="mass2" value="95" />
  <xacro:property name="mass3" value="71" />
  <xacro:property name="mass4" value="17" />
  <xacro:property name="mass5" value="7" />
  <xacro:property name="mass6" value="0.5" />
  <xacro:property name="gripper_mass" value="0.3" />
  <xacro:property name="finger_mass" value="0.1" />
  <xacro:property name="gripper_type" value="two_finger" /><!--value="vacuum" or "two_finger"-->
  <xacro:property name="vacuum_gripper_radius" value="0.1" />

  <!--Import gazebo elements-->
  <!-- <xacro:include filename="$(find kuka_arm)/urdf/kr210.gazebo.xacro" /> -->
  
  <link name="world"></link>


  <joint name="world_to_work_space" type="fixed">
      <parent link="world" />
      <child link="work_space" />
      <origin  rpy="1.57 0 0" xyz="0 0 0.015" />
  </joint>

  <link name="work_space">
      <visual>
          <origin xyz="0.2 0 0" rpy="0 0 0"/>
          <geometry>
              <box size="0.48 0.03 0.88" />
          </geometry>
          <!-- <material name="grey" /> -->
      </visual>
  </link>

  <joint name="base_joint" type="fixed">
    <parent link="work_space"/>
    <child link="base_link"/>
    <origin xyz="0 0.03 0" rpy="-1.57 0 0"/>
  </joint>

  <link name="base_link">
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="${mass0}"/>
      <inertia ixx="60" ixy="0" ixz="0" iyy="70" iyz="0" izz="100"/>
    </inertial>
    <visual>
      <origin xyz="0 0 0" rpy="${-pi/2} 0 0"/>
      <geometry>
        <box size="0.085 0.03 0.09" />
        <!-- <mesh filename="package://Sorting_Robot_Arm/meshes/kr210l150/visual/base_link.dae"/> -->
      </geometry>
      <material name="">
        <color rgba="0.75294 0.75294 0.75294 1"/>
      </material>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="${-pi/2} 0 0"/>
      <geometry>
        <box size="0.085 0.03 0.09" />
        <!-- <mesh filename="package://Sorting_Robot_Arm/meshes/kr210l150/collision/base_link.stl"/> -->
      </geometry>
    </collision>
  </link>


  <joint name="joint_1" type="revolute">
    <origin xyz="0.032 0 0.015" rpy="0 0 0"/>
    <parent link="base_link"/>
    <child link="link_1"/>
    <axis xyz="0 0 1"/>
    <limit lower="${-185*deg}" upper="${185*deg}" effort="300" velocity="${123*deg}"/>
  </joint>
 
  <link name="link_1">
    <inertial>
      <origin xyz="0 0 0.4" rpy="0 0 0"/>
      <mass value="${mass1}"/>
      <inertia ixx="30" ixy="0" ixz="0" iyy="50" iyz="0" izz="50"/>
    </inertial>
    <visual>
      <origin xyz="0 0 0.0325" rpy="0 0 0"/>
      <geometry>
        <cylinder length="0.065" radius="0.02" />
        <!-- <mesh filename="package://Sorting_Robot_Arm/meshes/kr210l150/visual/link_1.dae"/> -->
      </geometry>
      <material name="">
        <color rgba="0.75294 0.75294 0.75294 1"/>
      </material>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <cylinder length="0.065" radius="0.02" />
        <!-- <mesh filename="package://Sorting_Robot_Arm/meshes/kr210l150/collision/link_1.stl"/> -->
      </geometry>
    </collision>
  </link>

  <joint name="joint_2" type="revolute">
    <origin xyz="0 0 0.065" rpy="0 0 0"/>
    <parent link="link_1"/>
    <child link="link_2"/>
    <axis xyz="0 1 0"/>
    <limit lower="${-45*deg}" upper="${85*deg}" effort="300" velocity="${115*deg}"/>
  </joint>
  
  <link name="link_2">
    <inertial>
      <origin xyz="0 0 0.44751" rpy="0 0 0"/>
      <mass value="${mass2}"/>
      <inertia ixx="40" ixy="0" ixz="0" iyy="40" iyz="0" izz="10"/>
    </inertial>
    <visual>
      <origin xyz="0 0 0.0525" rpy="0 0 0"/>
      <geometry>
        <cylinder length="0.105" radius="0.02" />
        <!-- <mesh filename="package://Sorting_Robot_Arm/meshes/kr210l150/visual/link_2.dae"/> -->
      </geometry>
      <material name="">
        <color rgba="0.75294 0.75294 0.75294 1"/>
      </material>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <cylinder length="0.105" radius="0.02" />
        <!-- <mesh filename="package://Sorting_Robot_Arm/meshes/kr210l150/collision/link_2.stl"/> -->
      </geometry>
    </collision>
  </link>

  <joint name="joint_3" type="revolute">
    <origin xyz="0 0 0.105" rpy="0 0 0"/>
    <parent link="link_2"/>
    <child link="link_3"/>
    <axis xyz="0 1 0"/>
    <limit lower="${-210*deg}" upper="${(155-90)*deg}" effort="300" velocity="${112*deg}"/>
  </joint>
 
 
  <link name="link_3">
    <inertial>
      <origin xyz="0.18842 0.18344 -0.042799" rpy="0 0 0"/>
      <mass value="${mass3}"/>
      <inertia ixx="1" ixy="0" ixz="0" iyy="10" iyz="0" izz="10"/>
    </inertial>
    <visual>
      <origin xyz="0 0 0.075" rpy="0 0 0"/>
      <geometry>
        <cylinder length="0.15" radius="0.02" />
        <!-- <mesh filename="package://Sorting_Robot_Arm/meshes/kr210l150/visual/link_3.dae"/> -->
      </geometry>
      <material name="">
        <color rgba="0.75294 0.75294 0.75294 1"/>
      </material>
    </visual>
    <collision>
      <origin xyz="0 -0.184 0" rpy="0 0 0"/>
      <geometry>
        <cylinder length="0.15" radius="0.02" />
        <!-- <mesh filename="package://Sorting_Robot_Arm/meshes/kr210l150/collision/link_3.stl"/> -->
      </geometry>
    </collision>
  </link>

  <joint name="joint_4" type="revolute">
    <!-- <origin xyz="0.96 0 -0.054" rpy="0 0 0"/> -->
    <origin xyz="0 0 0.15" rpy="0 0 0"/>
    <parent link="link_3"/>
    <child link="link_4"/>
    <axis xyz="0 1 0"/>
    <limit lower="${-125*deg}" upper="${125*deg}" effort="300" velocity="${172*deg}"/>
  </joint>

  <link name="link_4">
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="${mass5}"/>
      <inertia ixx="0.04" ixy="0" ixz="0" iyy="0.068" iyz="0" izz="0.068"/>
    </inertial>
    <visual>
      <origin xyz="0 0 0.0375" rpy="0 0 0"/>
      <geometry>
        <cylinder length="0.075" radius="0.02" />
        <!-- <mesh filename="package://Sorting_Robot_Arm/meshes/kr210l150/visual/link_5.dae"/> -->
      </geometry>
      <material name="">
        <color rgba="0.75294 0.75294 0.75294 1"/>
      </material>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <cylinder length="0.075" radius="0.02" />
        <!-- <mesh filename="package://Sorting_Robot_Arm/meshes/kr210l150/collision/link_5.stl"/> -->
      </geometry>
    </collision>
  </link>

 
  <joint name="joint_5" type="revolute">
    <!-- <origin xyz="0.54 0 0" rpy="0 0 0"/> -->
    <origin xyz="0 0 0.075" rpy="0 0 0"/>
    <parent link="link_4"/>
    <child link="link_5"/>
     <axis xyz="0 0 1"/>
    <limit lower="${-350*deg}" upper="${350*deg}" effort="300" velocity="${219*deg}"/>
  </joint>

  <link name="link_5">
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="${mass6}"/>
      <inertia ixx="0.004" ixy="0" ixz="0" iyy="0.003" iyz="0" izz="0.003"/>
    </inertial>
    <visual>
      <origin xyz="0 0 0.0425" rpy="0 0 0"/>
      <geometry>
        <cylinder length="0.085" radius="0.02" />
        <!-- <mesh filename="package://Sorting_Robot_Arm/meshes/kr210l150/visual/link_6.dae"/> -->
      </geometry>
      <material name="">
        <color rgba="0.75294 0.75294 0.75294 1"/>
      </material>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <cylinder length="0.085" radius="0.04" />
        <!-- <mesh filename="package://Sorting_Robot_Arm/meshes/kr210l150/collision/link_6.stl"/> -->
      </geometry>
    </collision>
  </link>

  <!--Gripper links-->
  <!--Vacuum gripper-->
 
 
  <!--Transmission and actuators-->
  <transmission name="tran1">
    <type>transmission_interface/SimpleTransmission</type>
    <joint name="joint_1">
      <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
    </joint>
    <actuator name="motor1">
      <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
      <mechanicalReduction>1</mechanicalReduction>
    </actuator>
  </transmission>

  <transmission name="tran2">
    <type>transmission_interface/SimpleTransmission</type>
    <joint name="joint_2">
      <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
    </joint>
    <actuator name="motor2">
      <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
      <mechanicalReduction>1</mechanicalReduction>
    </actuator>
  </transmission>

  <transmission name="tran3">
    <type>transmission_interface/SimpleTransmission</type>
    <joint name="joint_3">
      <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
    </joint>
    <actuator name="motor3">
      <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
      <mechanicalReduction>1</mechanicalReduction>
    </actuator>
  </transmission>

  <transmission name="tran4">
    <type>transmission_interface/SimpleTransmission</type>
    <joint name="joint_4">
      <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
    </joint>
    <actuator name="motor4">
      <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
      <mechanicalReduction>1</mechanicalReduction>
    </actuator>
  </transmission>

  <transmission name="tran5">
    <type>transmission_interface/SimpleTransmission</type>
    <joint name="joint_5">
      <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
    </joint>
    <actuator name="motor5">
      <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
      <mechanicalReduction>1</mechanicalReduction>
    </actuator>
  </transmission>

 

</robot>